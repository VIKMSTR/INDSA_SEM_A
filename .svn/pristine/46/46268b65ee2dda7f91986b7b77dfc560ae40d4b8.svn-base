namespace INDSA_Sem_A
{
    public class Generator
    {
        private static int count = 0;
        public static string CreateUniqueID(NodeType nodeType)
        {
            count++;
            if (nodeType == NodeType.BusStop)
            {
                 return "Z" + count;
            }else if (nodeType == NodeType.RestingPlace)
            {
                return "O" + count;

            }
                return "K" + count;

        } 
    }
}